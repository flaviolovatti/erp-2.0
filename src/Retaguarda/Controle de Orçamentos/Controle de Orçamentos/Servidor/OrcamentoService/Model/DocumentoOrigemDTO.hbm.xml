<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="OrcamentoService"
                   namespace="OrcamentoService.Model" default-lazy="false">
  <class name="DocumentoOrigemDTO" table="fin_documento_origem">
    <id name="id" column="ID">
      <generator class="identity" />
    </id>
    <property name="idEmpresa" column="ID_EMPRESA" />
    <property name="siglaDocumento" column="SIGLA_DOCUMENTO" />
    <property name="descricao" column="DESCRICAO" />
  </class>
</hibernate-mapping>