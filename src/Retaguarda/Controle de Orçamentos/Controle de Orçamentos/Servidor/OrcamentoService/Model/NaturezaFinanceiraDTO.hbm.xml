<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="OrcamentoService"
                   namespace="OrcamentoService.Model" default-lazy="false">
  <class name="NaturezaFinanceiraDTO" table="natureza_financeira">
    <id name="id" column="ID">
      <generator class="identity" />
    </id>
    <property name="idPlanoNaturezaFinanceira" column="ID_PLANO_NATUREZA_FINANCEIRA" />
    <property name="descricao" column="DESCRICAO" />
    <property name="tipo" column="TIPO" />
    <property name="classificacao" column="CLASSIFICACAO" />
  </class>
</hibernate-mapping>