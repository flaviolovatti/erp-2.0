<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="OrcamentoService"
                   namespace="OrcamentoService.Model" default-lazy="false">
  <class name="OrcamentoPeriodoDTO" table="orcamento_periodo">
    <id name="id" column="ID">
      <generator class="identity" />
    </id>
    <property name="idEmpresa" column="ID_EMPRESA" />
    <property name="periodo" column="PERIODO" />
    <property name="nome" column="NOME" />
  </class>
</hibernate-mapping>